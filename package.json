{
  "name": "gps-navigation-app",
  "version": "1.0.0",
  "description": "GPS Navigation App with Custom Street Data",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently -k \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd web-app && npm run dev",
    "build": "cd web-app && npm install && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd web-app && npm install",
    "check-setup": "node check-setup.js"
  },
  "keywords": ["navigation", "maps", "custom-streets"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "body-parser": "^1.20.2",
    "better-sqlite3": "^9.4.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}

